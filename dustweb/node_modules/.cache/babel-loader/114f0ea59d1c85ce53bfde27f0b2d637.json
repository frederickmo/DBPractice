tent":["<template>\n  <el-table\n      :data=\"tableData\"\n      style=\"width: 100%\"\n      border>\n    <el-table-column type=\"expand\">\n      <template #default=\"props\" >\n        <el-form label-position=\"left\" inline class=\"demo-table-expand\">\n          <el-form-item label=\"运输编号\">\n            <span>{{ props.row.trans_id }}</span>\n          </el-form-item>\n          <el-form-item label=\"垃圾桶\">\n            <span>{{ props.row.dustbin_id }}</span>\n          </el-form-item>\n          <el-form-item label=\"运输车辆\">\n            <span>{{ props.row.truck_id }}</span>\n          </el-form-item>\n          <el-form-item label=\"运输状态\">\n            <span v-if=\"props.row.start_time!=props.row.end_time\">已结束</span>\n            <span v-else>运输中</span>\n          </el-form-item>\n          <el-form-item label=\"运输终点\">\n            <span>{{ props.row.plant_name }}</span>\n          </el-form-item>\n          <el-form-item label=\"开始时间\">\n            <span>{{ props.row.start_time }}</span>\n          </el-form-item>\n          <el-form-item label=\"结束时间\">\n            <span v-if=\"props.row.start_time!=props.row.end_time\">{{ props.row.end_time }}</span>\n          </el-form-item>\n        </el-form>\n      </template>\n    </el-table-column>\n    <el-table-column\n        label=\"运输编号\"\n        prop=\"trans_id\">\n    </el-table-column>\n    <el-table-column\n        label=\"开始时间\"\n        prop=\"start_time\">\n    </el-table-column>\n    <el-table-column\n        label=\"运输状态\">\n      <template #default=\"scope\">\n        <span v-if=\"scope.row.end_time === scope.row.start_time\">运输中</span>\n        <span v-else>已结束</span>\n      </template>\n    </el-table-column>\n    <el-table-column\n        fixed=\"right\"\n        label=\"操作\"\n        width=\"100\">\n      <template #default=\"scope\">\n        <el-button :disabled=\"scope.row.end_time != scope.row.start_time\" @click=\"TransportEnd(scope.row.trans_id)\" type=\"text\" >到站</el-button>\n      </template>\n    </el-table-column>\n  </el-table>\n</template>\n\n<script>\nimport {Base64} from 'js-base64';\n\nexport default {\n  data() {\n    return {\n      UserInfo: {\n        account: Base64.decode(localStorage.getItem(\"username\")),\n        name: '',\n        phone: '',\n        plant_name:'',\n        password: Base64.decode(localStorage.getItem(\"password\")),\n      },\n      TransportRecord: {\n        \"trans_id\": \"3952108210721191201\",\n        \"dustbin_id\": \"D002\",\n        \"truck_id\": \"T01\",\n        \"carrier_id\": \"3952108\",\n        \"plant_name\": \"\",\n        \"start_time\": \"2021-07-21T19:12:01\",\n        \"end_time\": \"2021-07-21T19:12:01\"\n      },\n      showDetail: false,\n      tableData: []\n    }\n  },\n  methods:{\n    getUserInfo() {\n      console.log(this.UserInfo.account, this.UserInfo.password);\n      fetch(this.$store.state.URL + \"/User/GetInformation/StationStaff?req=\" + this.UserInfo.account, {\n        method: \"GET\",\n        headers: {\n          \"Authorization\": \"Bearer \" + Base64.decode(localStorage.getItem(\"token\")),\n        },\n      }).then((response) => {\n        let result = response.json();\n        result.then((result) => {\n          this.UserInfo.name = result.name;\n          this.UserInfo.phone = result.phonenumber;\n          this.UserInfo.address = result.address;\n          this.UserInfo.plant_name=result.plantname;\n          this.getData();\n        })\n      })\n    },\n    getData(){\n      console.log(1,this.UserInfo.plant_name);\n      const url = this.$store.state.URL + \"/Transport/StaffGet?req=\" + this.UserInfo.plant_name;\n      fetch(url, {\n        method: \"GET\",\n        headers: {\n          accept: \"text/plain\",\n          Authorization: \"Bearer \" + Base64.decode(localStorage.token),\n        },\n      }).then((response) => {\n        console.log(response);\n        let result = response.json();\n        result.then((data) => {\n          console.log(data);\n          this.tableData = data;\n        });\n      })\n    },\n    TransportEnd(elm){\n      fetch(this.$store.state.URL + \"/Transport/TransportEnd?req=\" + elm, {\n        method: \"GET\",\n        headers: {\n          \"Authorization\": \"Bearer \" + Base64.decode(localStorage.getItem(\"token\")),\n        },\n      }).then((response) => {\n        let result = response.json();\n        result.then((result) => {\n          if(result.status===1)\n            this.getData();\n        })\n      })\n    }\n  },\n  mounted() {\n    console.log(1);\n    console.log(localStorage.token);\n    console.log(Base64.decode(localStorage.username));\n    this.getUserInfo();\n  }\n}\n</script>\n\n<style>\n.demo-table-expand {\n  font-size: 0;\n}\n\n.demo-table-expand label {\n  width: 90px;\n  color: #99a9bf;\n}\n\n.demo-table-expand .el-form-item {\n  margin-right: 0;\n  margin-bottom: 0;\n  width: 50%;\n}\n</style>\n"],"sourceRoot":""}]}